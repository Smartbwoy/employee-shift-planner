    "axios": "1.6.7",
    "bootstrap": "5.3.2",
    "date-fns": "2.30.0",
    "jspdf": "2.5.1",
    "jspdf-autotable": "3.8.1",
    "react": "18.2.0",
    "react-big-calendar": "1.8.5",
